{
  "src": "/api/**/*.js",
  "use": "@vercel/node"
},
{
  "src": "public/**/*",
  "use": "@vercel/static"
},
"routes": [
  {
    "src": "/api/(.*)",
    "dest": "/api/$1"
  },
  {
    "src": "/socket.io/(.*)",
    "dest": "/socket/server.js"
  },
  {
    "src": "/(.*)",
    "dest": "/public/$1"
  }
]
